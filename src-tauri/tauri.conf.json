{
	"build": {
		"beforeBuildCommand": "npm run build",
		"beforeDevCommand": "npm run dev",
		"devPath": "http://localhost:5173",
		"distDir": "../build"
	},
	"package": {
		"productName": "VELAppsHub",
		"version": "0.1.0"
	},
	"tauri": {
		"allowlist": {
			"fs": {
				"all": true,
				"scope": ["$APPCONFIG", "$APPCONFIG/*", "$APP/*", "$APP"]
			},
			"path": {
				"all": true
			},
			"os": {
				"all": true
			},
			"http": {
				"all": true,
				"request": true,
				"scope": ["https://**/*"]
			}
		},
		"bundle": {
			"active": true,
			"category": "DeveloperTool",
			"copyright": "",
			"deb": {
				"depends": []
			},
			"externalBin": [],
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "edu.uga.engr.vel.velappshub",
			"longDescription": "",
			"macOS": {
				"entitlements": null,
				"exceptionDomain": "",
				"frameworks": [],
				"providerShortName": null,
				"signingIdentity": null
			},
			"resources": [],
			"shortDescription": "",
			"targets": "all",
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 750,
				"resizable": true,
				"title": "VEL Apps Hub",
				"width": 1080
			}
		]
	}
}
